package me.inputignhere;

import org.bukkit.command.Command;
import org.bukkit.command.CommandSender;
import org.bukkit.entity.Player;
import org.bukkit.plugin.java.JavaPlugin;

public class inputfun extends JavaPlugin{
	public void onEnable() {
	}
	public void onDisable() {
	}
	public boolean onCommand(CommandSender sender, Command cmd, String label, String[] args) {
		if(label.equalsIgnoreCase("test")) {
			Player p = (Player) sender;
			p.sendMessage("&eHey! You just ran the test command!");
			p.sendMessage("&cThis plugin needs Essentials or a chat color plugin to work the best!"));
		}
		if(label.equalsIgnoreCase("ibelieveicanfly")) {
			Player p = (Player) sender;
			p.sendMessage("&aYou can now fly! enjoy!");
			p.getAllowFlight();
		}
		if(label.equalsIgnoreCase("expplz")) {
			Player p = (Player) sender;
			p.sendMessage("&bYou got &aexperience&b!!!");
			p.giveExpLevels(30);
		}
		if(label.equalsIgnoreCase("derpy")) {
			Player p = (Player) sender;
			p.sendMessage("You have been derpified!");
			p.addPotionEffect(9)
		}
		
		
		return true;
	}
}
